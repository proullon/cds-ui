{
  "generator-ovh-stack": {
    "generatorVersion": "0.0.0",
    "endpointDirectory": "server/api/",
    "insertRoutes": true,
    "registerRoutesFile": "server/routes.js",
    "routesNeedle": "// Insert routes below",
    "routesBase": "/api/",
    "pluralizeRoutes": true,
    "insertSockets": true,
    "registerSocketsFile": "server/config/socketio.js",
    "socketsNeedle": "// Insert sockets below",
    "insertModels": true,
    "registerModelsFile": "server/sqldb/index.js",
    "modelsNeedle": "// Insert models below",
    "filters": {
      "public": true,
      "advanced": true,
      "js": true,
      "notV6": true,
      "front": true,
      "back": true,
      "frontback": true,
      "es5": true,
      "html": true,
      "less": true,
      "uirouter": true,
      "html5": false,
      "ngresource": true,
      "bootstrap": true,
      "uibootstrap": true,
      "toaster": true,
      "moment": true,
      "ovhcommonstyle": true,
      "swimmingpoll": true,
      "ovhhttp": true,
      "uivalidate": true,
      "atinternet": true,
      "i18n": true,
      "auth": true,
      "models": true,
      "mongooseModels": true,
      "mongoose": true,
      "localAuth": true,
      "grunt": true,
      "gulp": false,
      "jasmine": true,
      "mocha": false,
      "should": false,
      "expect": false
    }
  },
  "generator-ovh-component": {
    "routeDirectory": "client/app/",
    "directiveDirectory": "client/app/",
    "filterDirectory": "client/app/",
    "serviceDirectory": "client/app/",
    "basePath": "client",
    "uistatePrefix": "app",
    "moduleName": "",
    "modulePrompt": false,
    "filters": [
      "uirouter",
      "jasmine"
    ],
    "extensions": [
      "js",
      "html",
      "less"
    ],
    "directiveSimpleTemplates": "",
    "directiveComplexTemplates": "",
    "filterTemplates": "",
    "serviceTemplates": "",
    "factoryTemplates": "",
    "controllerTemplates": "",
    "decoratorTemplates": "",
    "providerTemplates": "",
    "routeTemplates": ""
  }
}